    private mutating func changeSticker(_ action: StickerModification,
                                        id: String,
                                        in collage: Collage) -> Collage {
        
        guard let stickerIndex = collage.stickers.firstIndex(where: {
            $0.id == id
        }) else {
            return collage
        }
    private mutating func onRotate(_ gestureState: GestureType.GestureState<CGFloat>,
                                   in state: AppState) -> AppState {
        
        guard state.editMode == .preview else {
            return state
        }
